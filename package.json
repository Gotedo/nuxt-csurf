{
  "name": "@gotedo/nuxt-csurf",
  "version": "1.0.0-1",
  "description": "Nuxt Cross-Site Request Forgery (CSRF) Prevention",
  "keywords": [
    "nuxt",
    "csrf",
    "module"
  ],
  "author": "morgbn",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "lint": "eslint --ext .js,.ts,.vue .",
    "prettier": "prettier . --write --cache  --ignore-unknown",
    "prepack": "yarn build",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepare": "husky install",
    "release": "np"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.1",
    "defu": "^6.1.1",
    "uncsrf": "^1.1.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.1",
    "@types/node": "^18.17.14",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "np": "^8.0.4",
    "nuxt": "^3.7.1",
    "prettier": "3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gotedo/nuxt-csurf.git"
  },
  "contributors": [
    {
      "name": "Ndianabasi Udonkang",
      "url": "https://github.com/ndianabasi"
    }
  ],
  "volta": {
    "node": "20.9.0",
    "yarn": "4.0.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --cache --ignore-unknown"
  },
  "np": {
    "yarn": false,
    "yolo": true,
    "contents": ".",
    "anyBranch": false,
    "publish": false,
    "branch": "main",
    "releaseDraft": true,
    "message": "chore: bump version to %s ðŸš€"
  }
}
